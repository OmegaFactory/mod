	sounds[]: "stick_hazard_warning|/def/vehicle/truck/common/interior/zm_fh4_stick_hazard_warning.soundref"
	sounds[]: "stick_high_beam|/def/vehicle/truck/common/interior//zm_fh4_stick_blinker.soundref"
	sounds[]: "stick_light_horn|/def/vehicle/truck/common/interior/zm_fh4_stick_blinker.soundref"
	sounds[]: "stick_lights|/def/vehicle/truck/common/interior/zm_fh4_stick_lights.soundref"
